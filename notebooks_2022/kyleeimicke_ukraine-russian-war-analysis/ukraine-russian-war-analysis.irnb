{"metadata":{"kernelspec":{"name":"ir","display_name":"R","language":"R"},"language_info":{"name":"R","codemirror_mode":"r","pygments_lexer":"r","mimetype":"text/x-r-source","file_extension":".r","version":"4.0.5"}},"nbformat_minor":4,"nbformat":4,"cells":[{"cell_type":"code","source":"## read csv.files on Ukraine_Russian_War\nlost_equipment <- read.csv(\"../input/2022-ukraine-russian-war/russia_losses_equipment.csv\")\nlost_personnel <- read.csv(\"../input/2022-ukraine-russian-war/russia_losses_personnel.csv\")","metadata":{"_uuid":"a186614d-e5e3-4c4e-82de-0bc5eba9e4e3","_cell_guid":"c2c7c6b9-0d8f-4d66-bdbe-38d4295b3515","execution":{"iopub.status.busy":"2022-05-08T12:44:35.261966Z","iopub.execute_input":"2022-05-08T12:44:35.263256Z","iopub.status.idle":"2022-05-08T12:44:35.282083Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"##intstalling necessary packages to use for analysis\ninstall.packages(\"tidyverse\")\ninstall.packages(\"ggrepel\")\ninstall.packages(\"viridis\")\nlibrary(\"tidyverse\")\nlibrary(\"ggplot2\")\nlibrary(\"lubridate\")\nlibrary(\"geosphere\")\nlibrary(\"gridExtra\") \nlibrary(\"ggmap\") \nlibrary(\"janitor\")\nlibrary(\"ggrepel\")\nlibrary(\"viridis\")\n","metadata":{"_uuid":"46263736-4f7e-472b-ab77-dd2e3a11d564","_cell_guid":"5d4bf0c7-2594-4d60-8ca0-dbcdc90b624b","execution":{"iopub.status.busy":"2022-05-08T12:44:35.28443Z","iopub.execute_input":"2022-05-08T12:44:35.28576Z","iopub.status.idle":"2022-05-08T12:45:15.402709Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Adjust all Plot sizes to fit screen and be readable\noptions(repr.plot.width = 20, repr.plot.height = 10)","metadata":{"execution":{"iopub.status.busy":"2022-05-08T12:45:15.406082Z","iopub.execute_input":"2022-05-08T12:45:15.407574Z","iopub.status.idle":"2022-05-08T12:45:15.420799Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Preview lost equipment dataset in war using head and structure functions \nhead(lost_equipment)\nstr(lost_equipment)","metadata":{"_uuid":"d598bdae-3dd0-4cb0-8e05-6cf98d0cbc55","_cell_guid":"21ee28dd-449e-4318-bae9-78b696fe0b3f","execution":{"iopub.status.busy":"2022-05-08T12:45:15.423565Z","iopub.execute_input":"2022-05-08T12:45:15.424971Z","iopub.status.idle":"2022-05-08T12:45:15.468865Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Preview of lost personnel of war using head and glimpse functions\nhead(lost_personnel)\nglimpse(lost_personnel)","metadata":{"_uuid":"0342e483-44b4-4f24-a0e8-66b2d03afb2e","_cell_guid":"39eb7678-5771-4b37-8351-bff1cbd0eb40","execution":{"iopub.status.busy":"2022-05-08T12:45:15.470967Z","iopub.execute_input":"2022-05-08T12:45:15.472245Z","iopub.status.idle":"2022-05-08T12:45:15.499665Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## dropping all N/A values from both datasets if present \nlost_equipment[is.na(lost_equipment)] <- 0;\nlost_personnel[is.na(lost_personnel)] <- 0","metadata":{"_uuid":"779074eb-ff86-4007-a509-b94d5bf453f3","_cell_guid":"3987dc27-d16b-48a1-a9df-cfb9a64ae8c2","execution":{"iopub.status.busy":"2022-05-08T12:45:15.501726Z","iopub.execute_input":"2022-05-08T12:45:15.502957Z","iopub.status.idle":"2022-05-08T12:45:15.516669Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"##clean names in each dataset to show uniformity and make analysis easier\nlost_equipment <- clean_names(lost_equipment)\nlost_personnel <-clean_names(lost_personnel);","metadata":{"_uuid":"48952a47-fb20-45c7-ac45-463de509ad0a","_cell_guid":"3988a94c-aaad-4c7c-bf48-57f8a2a79c39","execution":{"iopub.status.busy":"2022-05-08T12:45:15.518808Z","iopub.execute_input":"2022-05-08T12:45:15.52006Z","iopub.status.idle":"2022-05-08T12:45:15.558987Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"##Correct Date value from chr to date\nlost_equipment <- lost_equipment %>%\n    mutate(date = ymd(date));\n\nlost_personnel <- lost_personnel %>%\n    mutate(date = ymd(date))","metadata":{"_uuid":"cf72ce2d-692d-451c-b244-ea7ee88f33c6","_cell_guid":"cf4ede14-a071-4e78-9996-90bbbf08a7fc","execution":{"iopub.status.busy":"2022-05-08T12:45:15.561181Z","iopub.execute_input":"2022-05-08T12:45:15.562481Z","iopub.status.idle":"2022-05-08T12:45:15.587077Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Create a  new column for daily death count of Russian Soldiers, and run summary function\n## to show average daily death rate\nlost_personnel <- lost_personnel %>%\n    mutate(deaths_per_day = personnel - lag(personnel, default = personnel[1]))\n\nsummary(lost_personnel)","metadata":{"_uuid":"0fca9e8c-e109-49bc-bb0e-d9bfce5fe3e7","_cell_guid":"158049de-ade7-4437-8f61-85a72b3fef13","execution":{"iopub.status.busy":"2022-05-08T12:45:15.589283Z","iopub.execute_input":"2022-05-08T12:45:15.590547Z","iopub.status.idle":"2022-05-08T12:45:15.611279Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## A graph that shows the Total reported deaths and POWs of Russian Soliders. \nggplot(lost_personnel) +\n    geom_smooth(mapping = aes(x = date, y = personnel), \n                color = \"darkred\", size = 2, se = FALSE) +\n    geom_smooth(mapping = aes(x = date, y = pow), \n                color = \"black\", size = 2, se = FALSE) +\nlabs(title = \"TOTAL REPORTED DEATHS OF RUSSIAN SOLDIERS \",\n     x = \"DATE IN WAR\", y = \"TOTAL PEOPLE LOST\") +\n        theme_bw() +\n            theme(plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                    axis.text = element_text(face = \"bold\",size = 18),\n                        axis.title.y = element_text(face = \"bold\", size = 18),\n                            axis.title.x = element_text(face = \"bold\", size = 18))","metadata":{"_uuid":"2c7beead-6aca-4f4b-9ac6-f01c0d0a6ec7","_cell_guid":"d2153ca5-cb78-4ffb-af26-af8d76a1bd6e","execution":{"iopub.status.busy":"2022-05-08T12:45:15.613543Z","iopub.execute_input":"2022-05-08T12:45:15.614834Z","iopub.status.idle":"2022-05-08T12:45:15.968299Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Plot of reported daily death count for Russian Forces\nlost_personnel %>%\n    ggplot(mapping = aes(x = day, y = deaths_per_day)) +\n        geom_col(fill = \"darkred\", color = \"black\") +\n            geom_label(aes(label = deaths_per_day),\n                            color = \"black\", size = 6) +\nlabs(title = \"REPORTED DAILY DEATHS OF RUSSIAN PERSONNEL\",\n     x = \"DAY OF WAR\", y = \"DEATHS PER DAY\") +\n    theme_bw() +\n        theme(\n            plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                axis.text = element_text(face = \"bold\",size = 18),\n                    axis.title.y = element_text(face = \"bold\", size = 18),\n                        axis.title.x = element_text(face = \"bold\", size = 18))","metadata":{"_uuid":"288ba22a-3de6-4310-8b89-6ae401a17082","_cell_guid":"c63470fb-29a5-43c4-874c-85a4b9f5e080","execution":{"iopub.status.busy":"2022-05-08T12:45:15.970525Z","iopub.execute_input":"2022-05-08T12:45:15.972083Z","iopub.status.idle":"2022-05-08T12:45:16.746231Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Dataset changed how it measures columns so we will mutate for the columns for accuaracy\n## It also stopped recording the mobile_srbm_system column and so that was removed as it\n## recorded under 10 lost and will allow for cleaner analysis later\nlost_equipment <- lost_equipment %>%\n    mutate(vehicles_and_fuel_tanks = fuel_tank + military_auto + vehicles_and_fuel_tanks,\n          fuel_tank = NULL, military_auto = NULL, mobile_srbm_system = NULL)","metadata":{"_uuid":"75ad0a84-02da-4b8c-b437-156cfaa2041a","_cell_guid":"5ec11180-966f-4ae8-9bbc-e27bc5f939c4","execution":{"iopub.status.busy":"2022-05-08T12:45:16.748656Z","iopub.execute_input":"2022-05-08T12:45:16.750044Z","iopub.status.idle":"2022-05-08T12:45:16.765627Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Convert data to long format to prepare for analysis using different graphs\nlost_equipment_long <- lost_equipment %>%\n    pivot_longer(cols = aircraft:cruise_missiles, \n                 names_to = \"equipment\", values_to = \"losses\")\n","metadata":{"_uuid":"a7de8fdb-82d5-489f-8d48-ab83454773c5","_cell_guid":"a2a3c442-cf05-4bad-8575-44755f5e422c","execution":{"iopub.status.busy":"2022-05-08T12:45:16.767924Z","iopub.execute_input":"2022-05-08T12:45:16.769319Z","iopub.status.idle":"2022-05-08T12:45:16.787203Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## long format is totaled While this is always the last updated row in the original dataset\n## this format allows us to use the code at anytime the dataset is downloaded and creates\n## the most up to date information\nlost_equipment_total <- lost_equipment_long %>%\n    group_by(equipment) %>%\n    summarise(total = max(losses, na.rm = TRUE))","metadata":{"_uuid":"36cdbdfe-fa3f-454c-806f-ebd710ed637e","_cell_guid":"2a247038-6bbd-4b72-a16f-d0ae63517f36","execution":{"iopub.status.busy":"2022-05-08T12:45:16.789463Z","iopub.execute_input":"2022-05-08T12:45:16.790825Z","iopub.status.idle":"2022-05-08T12:45:16.808479Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## A bar chart of equipment lost so far in the war \nggplot(lost_equipment_total, mapping = aes(x = total, y = equipment, fill = equipment)) +\n    geom_bar(stat = \"identity\", width = 0.5) +\n        scale_color_brewer(palette = \"Paired\") +\n            geom_label(aes(label = total), \n               color = \"black\", fontface = \"bold\", nudge_y = 0.25, alpha = 0.2, size = 10) +\nlabs(title = \"TYPE OF REPORTED EQUIPMENT LOST BY RUSSIA\", \n     x = \"NUMBERS LOST\", y = \"EQUIPMENT TYPE\") +\n        theme_bw() +\n            theme(legend.position = \"none\",\n                      plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                        axis.text = element_text(face = \"bold\",size = 20),\n                            axis.title.y = element_text(face = \"bold\", size = 18),\n                                axis.title.x = element_text(face = \"bold\", size = 18)) \n                ","metadata":{"_uuid":"67c43880-65aa-40ac-bcfb-691dab9574b8","_cell_guid":"bc3625a3-572a-4ce2-be0f-d12be87d1837","execution":{"iopub.status.busy":"2022-05-08T12:45:16.810865Z","iopub.execute_input":"2022-05-08T12:45:16.812267Z","iopub.status.idle":"2022-05-08T12:45:17.221409Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## This graph shows all of the equipment on a timeline. Since the scale of most equipment\n## lost is so vast I will split them up to show a better visualization and use different\n## color palettes\nlost_equipment_long %>%\n    ggplot(mapping = aes(x = date, y = losses, group = equipment, color = equipment)) +\n        geom_smooth(se = FALSE) + \n            guides(colour = guide_legend(nrow = 1, override.aes = list(size = 4))) +\n                scale_color_brewer(palette = \"Paired\") +\nlabs(title = \"APCs ARE THE MOST EQUIPMENT LOST OUTPACING THE NEXT ONE ALMOST 2:1\",\n    x = \"DATE\", y = \"TOTAL LOSSES\") +\n        theme_linedraw() +\n            theme(legend.position = \"bottom\",\n                    plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                        axis.text = element_text(face = \"bold\",size = 18),\n                            axis.title.y = element_text(face = \"bold\", size = 18),\n                                axis.title.x = element_text(face = \"bold\", size = 18),\n                                    legend.text = element_text(face = \"bold\", size = 15))\n        \n        ","metadata":{"_uuid":"b943fa6b-367a-4f60-8cf2-c11c49a83aaa","_cell_guid":"f7b6c8ee-bb61-4fd0-9b34-e3636ae28a96","execution":{"iopub.status.busy":"2022-05-08T12:45:17.22368Z","iopub.execute_input":"2022-05-08T12:45:17.224984Z","iopub.status.idle":"2022-05-08T12:45:17.903386Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## A facet_wrap shows the equipment lost in individual plots\nggplot(lost_equipment_long) +\n    geom_line(mapping = aes(x = date, y = losses, group = equipment, color = equipment)) +\n        facet_wrap(~equipment) +\nscale_color_brewer(palette = \"Paired\") +\n            theme(legend.position = \"none\",\n                    plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                        axis.text = element_text(face = \"bold\",size = 18),\n                            axis.title.y = element_text(face = \"bold\", size = 18),\n                                axis.title.x = element_text(face = \"bold\", size = 18))\n\n","metadata":{"execution":{"iopub.status.busy":"2022-05-08T12:45:17.905733Z","iopub.execute_input":"2022-05-08T12:45:17.907091Z","iopub.status.idle":"2022-05-08T12:45:18.848568Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Next step is to Show the majority and minority of equipment lost so far.\n## This needs to be created into two separate tables using the select function.\nmajority_lost_equipment <- lost_equipment %>%\nselect(date,day,apc,vehicles_and_fuel_tanks,tank,field_artillery);\n\nminority_lost_equipment <- lost_equipment %>%\nselect(date,day,aircraft,helicopter,mrl,drone,naval_ship,\n           anti_aircraft_warfare,special_equipment,cruise_missiles)\n\n","metadata":{"execution":{"iopub.status.busy":"2022-05-08T12:45:18.852117Z","iopub.execute_input":"2022-05-08T12:45:18.854291Z","iopub.status.idle":"2022-05-08T12:45:18.880702Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## The two tables are than put into a long format\nmajority_lost_equipment_long <- majority_lost_equipment %>%\n    pivot_longer(cols = apc:field_artillery,\n                names_to = \"equipment\", values_to = \"losses\");\n\nminority_lost_equipment_long <- minority_lost_equipment %>%\n    pivot_longer(cols = aircraft:cruise_missiles,\n                names_to = \"equipment\", values_to = \"losses\")","metadata":{"execution":{"iopub.status.busy":"2022-05-08T12:45:18.883534Z","iopub.execute_input":"2022-05-08T12:45:18.884975Z","iopub.status.idle":"2022-05-08T12:45:18.908378Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"## Both new tables of equipment are used to create a line graph showing equipment lost in a\n## easier format to read showing the differences using two different color-blind friendly\n## scales to express different colors\nmajority_lost_equipment_long %>%\n    ggplot(mapping = aes(x = date, y = losses, group = equipment, color = equipment)) +\n        geom_smooth(se = FALSE) + \n            guides(colour = guide_legend(nrow = 1, override.aes = list(size = 4))) +\n                scale_color_brewer(palette = \"Set1\") +\nlabs(title = \"THE TOP 4 PIECES OF EQUIPMENT LOST IN THE WAR\",\n        x = \"DATE\", y = \"TOTAL LOSSES\") +\n        theme_linedraw() +\n            theme(legend.position = \"bottom\",\n                plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                    axis.text = element_text(face = \"bold\",size = 18),\n                        axis.title.y = element_text(face = \"bold\", size = 18),\n                            axis.title.x = element_text(face = \"bold\", size = 18),\n                             legend.text = element_text(face = \"bold\", size = 20));\n\nminority_lost_equipment_long %>%\n    ggplot(mapping = aes(x = date, y = losses, group = equipment, color = equipment)) +\n        geom_smooth(se = FALSE) + \n            guides(colour = guide_legend(nrow = 1, override.aes = list(size = 4))) +\n                scale_color_brewer(palette = \"Dark2\") +\nlabs(title = \"REMAINING PIECES OF EQUIPMENT LOST IN THE WAR\",\n    x = \"DATE\", y = \"TOTAL LOSSES\") +\n        theme_linedraw() +\n            theme(legend.position = \"bottom\",\n                plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                    axis.text = element_text(face = \"bold\",size = 18),\n                        axis.title.y = element_text(face = \"bold\", size = 18),\n                            axis.title.x = element_text(face = \"bold\", size = 18),\n                             legend.text = element_text(face = \"bold\", size = 20));\n\n","metadata":{"execution":{"iopub.status.busy":"2022-05-08T12:45:18.910794Z","iopub.execute_input":"2022-05-08T12:45:18.912218Z","iopub.status.idle":"2022-05-08T12:45:19.955654Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"##This is a summation of all the equipment Russia has lost to date\ntotal_lost_equipment <- lost_equipment_long %>%\n    group_by(date) %>%\n      summarise(values = sum(losses,\n                            na.rm = TRUE))\n\nggplot(total_lost_equipment) +\n    geom_smooth(mapping = aes(x = date, y = values),size = 2, color = \"darkblue\") +\nlabs(title = \"THE TOTAL AMOUNT OF EQUIPMENT LOST BY RUSSIA IN THE WAR TO DATE\",\n        x = \"DATE\", y = \"TOTAL LOSSES\") + \n                theme(plot.title = element_text(face = \"bold\", size = 24, hjust = 0.5),\n                    axis.text = element_text(face = \"bold\",size = 18),\n                        axis.title.y = element_text(face = \"bold\", size = 18),\n                            axis.title.x = element_text(face = \"bold\", size = 18))","metadata":{"execution":{"iopub.status.busy":"2022-05-08T12:45:19.959216Z","iopub.execute_input":"2022-05-08T12:45:19.961331Z","iopub.status.idle":"2022-05-08T12:45:20.322445Z"},"trusted":true},"execution_count":null,"outputs":[]}]}