]},{"cell_type":"code","source":"test <- read.csv(\"../input/house-price-prediction-of-indore-3gb11/IndoreHP_Test.csv\")\ndata <- read.csv(\"../input/house-price-prediction-of-indore-3gb11/IndoreHP_Train.csv\")\nsample <- read.csv(\"../input/house-price-prediction-of-indore-3gb11/IndoreHP_Sample.csv\")","metadata":{"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"library(dplyr)\nlibrary(ggplot2)\nlibrary(car)\nlibrary(Metrics)\n\nhead(data)\n\n##Do Exploratory Analysis\n\n#CRIM: per capita crime rate\nsummary(data$CRIM)\nplot(data$CRIM,data$MEDV,col='red')\n# As crime rate increases, median value of house tends to fall but it is not a clear pattern (Heteroscedasticity??)\n\n#ZN:The proportion of residential land zoned for lots larger than 25,000 sq.ft.\nsummary(data$ZN)\nplot(data$ZN,data$MEDV,col='red')\n# No clear trend\n# Data might have heteroscedasticity\n\n#INDUS: The proportion of non-retail business acres per town\nsummary(data$INDUS)\nplot(data$INDUS,data$MEDV,col='red')\n# As proportion of non-retail business acres per town increases, median value tends to fall\n\n#RIVER_FLG:River Dummy Var (this is equal to 1 if tract bounds river; 0 otherwise)\nsummary(data$RIVER_FLG)\nplot(data$RIVER_FLG,data$MEDV,col='red')\n#categoric variable\n\n#nitric.oxides.concentration:the nitric oxides concentration (parts per 10 million)\nsummary(data$nitric.oxides.concentration)\nplot(data$nitric.oxides.concentration,data$MEDV,col='red')\n#heteroscedasticity\n\n#X.rooms.dwelling:the average number of rooms per dwelling\nsummary(data$X.rooms.dwelling)\nplot(data$X.rooms.dwelling,data$MEDV,col='red')\n# As no of rooms increases, median price tends to increase\n\n#AGE: the proportion of owner-occupied units built prior to 1940\nsummary(data$AGE)\nplot(data$AGE,data$MEDV,col='red')\n# As age increases, median value tends to fall\n\n#DIS: the weighted distances to five employment centers of the town\nsummary(data$DIS)\nplot(data$DIS,data$MEDV,col='red')\n\n#RAD: the index of accessibility to radial highways\nsummary(data$RAD)\nplot(data$RAD,data$MEDV,col='red')\n#Categorical\n\n#TAX: the full-value property-tax rate per INR 10 millions (1 crores)\nsummary(data$TAX)\nplot(data$TAX,data$MEDV,col='red')\n\n#PTRATIO: the pupil-teacher ratio by area\nsummary(data$PTRATIO)\nplot(data$PTRATIO,data$MEDV,col='red')\n\n#B: calculated as 1000(Bk - 0.63)Â², where Bk is the proportion of people living is slums in the area\nsummary(data$B)\nplot(data$B,data$MEDV,col='red')\n\n#LSTAT: This is the percentage lower status and income of the population\nsummary(data$LSTAT)\nplot(data$LSTAT,data$MEDV,col='red')\n\n##Linear Regression: MODEL 1\nmod1<-lm(MEDV~.,data=data)\nsummary(mod1)\n#Drop INDUS, RAD and RIVER_FLG as it seems to be insignificant\n\n##Linear Regression: MODEL 2\nmod2<-lm(formula = MEDV ~ CRIM + ZN + nitric.oxides.concentration + X.rooms.dwelling + AGE + DIS + TAX + PTRATIO + B + LSTAT, data = data)\nsummary(mod2)\n#Drop ZN as it is insignificant\n\n##Linear Regression: MODEL 3\nmod3<-lm(formula = MEDV ~ CRIM + nitric.oxides.concentration + X.rooms.dwelling + AGE + DIS + TAX + PTRATIO + B + LSTAT, data = data)\nsummary(mod3)\n#Now all the variables are significant\n\n##ASSUMPTION CHECKS\n\n#Normality Checks\nhist(mod3$residuals)\n\nqqPlot(mod3$residuals)\n#Mostly all are on 45 degree line\n\n#Multicollinearity Check\nvif(mod3)\n#Since all values are less than 10, there is no multicollinearity\n\n#Constant Variance Check\nplot(mod3$fitted.values,mod3$residuals) \n#No clear funneling, hence, we can assume there is no heteroscedasticity\n\n#Plot predicted values and actual values\npredicted<-mod3$fitted.values\nactual<-sqrt(data$MEDV)\n\ndat<-data.frame(predicted,actual)\n\n#Predicted Values\ndata$predicted.MEDV = predict(mod3, data)\n\nwrite.table(data,file=\"IndoreCaseStudyDataTRAIN1.csv\",sep=\",\")\n\nsubset(data,select=-c(predicted.MEDV))\n\n##REDOING USING TEST DATA##\ntest$predicted.MEDV = predict(mod3, test)\nwrite.table(test, file=\"IndoreCaseStudyDataTEST1.csv\",sep=\",\")\n\n# Check the RMSE score of test dataset\nrmse(test$predicted.MEDV, predict(mod3,test))\n\n##Linear Regression: MODEL 4\nmod4<-lm(formula = MEDV ~ CRIM + nitric.oxides.concentration + X.rooms.dwelling + DIS + TAX + PTRATIO + B + LSTAT, data = data)\nsummary(mod4)\n\n##ASSUMPTION CHECKS\n\n#Normality Checks\nhist(mod4$residuals)\n\nqqPlot(mod4$residuals)\n#Mostly all are on 45 degree line\n\n#Multicollinearity Check\nvif(mod4)\n#Since all values are less than 10, there is no multicollinearity\n\n#Constant Variance Check\nplot(mod4$fitted.values,mod4$residuals) \n#No clear funneling, hence, we can assume there is no heteroscedasticity\n\n#Plot predicted values and actual values\npredicted<-mod4$fitted.values\nactual<-sqrt(data$MEDV)\n\ndat<-data.frame(predicted,actual)\n\n#Predicted Values\ndata$predicted.MEDV = predict(mod4, data)\n\nwrite.table(data,file=\"IndoreCaseStudyDataTRAIN2.csv\",sep=\",\")\n\nsubset(data,select=-c(predicted.MEDV))\n\n##REDOING USING TEST DATA##\ntest$predicted.MEDV = predict(mod4, test)\nwrite.table(test, file=\"IndoreCaseStudyDataTEST2.csv\",sep=\",\")\n\n# Check the RMSE score of test dataset\nrmse(test$predicted.MEDV, predict(mod4,test))\n\n##Linear Regression: MODEL 5\nmod5<-lm(formula = MEDV ~ nitric.oxides.concentration + X.rooms.dwelling + DIS + TAX + PTRATIO + B + LSTAT, data = data)\nsummary(mod5)\n#Now all the variables are significant\n\n##ASSUMPTION CHECKS\n\n#Normality Checks\nhist(mod5$residuals)\n\nqqPlot(mod5$residuals)\n#Mostly all are on 45 degree line\n\n#Multicollinearity Check\nvif(mod5)\n#Since all values are less than 10, there is no multicollinearity\n\n#Constant Variance Check\nplot(mod5$fitted.values,mod5$residuals) \n#No clear funneling, hence, we can assume there is no heteroscedasticity\n\n#Plot predicted values and actual values\npredicted<-mod5$fitted.values\nactual<-sqrt(data$MEDV)\n\ndat<-data.frame(predicted,actual)\n\n#Predicted Values\ndata$predicted.MEDV = predict(mod5, data)\n\nwrite.table(data,file=\"IndoreCaseStudyDataTRAIN3.csv\",sep=\",\")\n\nsubset(data,select=-c(predicted.MEDV))\n\n##REDOING USING TEST DATA##\ntest$predicted.MEDV = predict(mod5, test)\nwrite.table(test, file=\"IndoreCaseStudyDataTEST3.csv\",sep=\",\")\n\n# Check the RMSE score of test dataset\nrmse(test$predicted.MEDV, predict(mod5,test))\n\n##Linear Regression:MODEL 6\n#Breush Pagan Test\nlibrary(lmtest)\nbptest(mod3)\n#Since p-value is less than 0.05, there is heteroskedasticity\n\n#Correcting Heteroskedasticity\nmod3$resi <- mod3$residuals\nvarfunc.mod3 <- lm(log(mod3$resi^2) ~ log(CRIM) + log(nitric.oxides.concentration) + log(X.rooms.dwelling) + log(AGE) + log(DIS) + log(TAX) + log(PTRATIO) + log(B) + log(LSTAT), data = data)\nmod3$varfunc <- exp(varfunc.mod3$fitted.values)\nmod3.gls <- lm(MEDV ~ CRIM + nitric.oxides.concentration + X.rooms.dwelling + AGE + DIS + TAX + PTRATIO + B + LSTAT, weights = 1/sqrt(mod3$varfunc), data = data)\n\nsummary(mod3)\nsummary(mod3.gls)\n\n#Checking assumptions\n##ASSUMPTION CHECKS\n\n#Normality Checks\nhist(mod3.gls$residuals)\n\nqqPlot(mod3.gls$residuals)\n#Mostly all are on 45 degree line\n\n#Multicollinearity Check\nvif(mod3.gls)\n#Since all values are less than 10, there is no multicollinearity\n\n#Constant Variance Check\nplot(mod3.gls$fitted.values,mod3.gls$residuals) \n#No clear funneling, hence, we can assume there is no heteroscedasticity\n\n#Plot predicted values and actual values\npredicted<-mod3.gls$fitted.values\nactual<-sqrt(data$MEDV)\n\ndat<-data.frame(predicted,actual)\n\n#Predicted Values\ndata$predicted.MEDV = predict(mod3.gls, data)\n\nwrite.table(data,file=\"IndoreCaseStudyDataTRAIN4.csv\",sep=\",\")\n\nsubset(data,select=-c(predicted.MEDV))\n\n##REDOING USING TEST DATA##\ntest$predicted.MEDV = predict(mod3.gls,test)\nwrite.table(test, file=\"IndoreCaseStudyDataTEST4.csv\",sep=\",\")\n\n# Check the RMSE score of test dataset\nrmse(test$predicted.MEDV, predict(mod3.gls,test))\n\n##Linear Regression:MODEL 7\n#Breush Pagan Test\nlibrary(lmtest)\nbptest(mod4)\n#Since p-value is less than 0.05, there is heteroskedasticity\n\n#Correcting Heteroskedasticity\nmod4$resi <- mod4$residuals\nvarfunc.mod4 <- lm(log(mod4$resi^2) ~ log(CRIM) + log(nitric.oxides.concentration) + log(X.rooms.dwelling) + log(DIS) + log(TAX) + log(PTRATIO) + log(B) + log(LSTAT), data = data)\nmod4$varfunc <- exp(varfunc.mod4$fitted.values)\nmod4.gls <- lm(MEDV ~ CRIM + nitric.oxides.concentration + X.rooms.dwelling + DIS + TAX + PTRATIO + B + LSTAT, weights = 1/sqrt(mod4$varfunc), data = data)\n\nsummary(mod4)\nsummary(mod4.gls)\n\n#Checking assumptions\n##ASSUMPTION CHECKS\n\n#Normality Checks\nhist(mod4.gls$residuals)\n\nqqPlot(mod4.gls$residuals)\n#Mostly all are on 45 degree line\n\n#Multicollinearity Check\nvif(mod4.gls)\n#Since all values are less than 10, there is no multicollinearity\n\n#Constant Variance Check\nplot(mod4.gls$fitted.values,mod4.gls$residuals) \n#No clear funneling, hence, we can assume there is no heteroscedasticity\n\n#Plot predicted values and actual values\npredicted<-mod4.gls$fitted.values\nactual<-sqrt(data$MEDV)\n\ndat<-data.frame(predicted,actual)\n\n#Predicted Values\ndata$predicted.MEDV = predict(mod4.gls, data)\n\nwrite.table(data,file=\"IndoreCaseStudyDataTRAIN5.csv\",sep=\",\")\n\nsubset(data,select=-c(predicted.MEDV))\n\n##REDOING USING TEST DATA##\ntest$predicted.MEDV = predict(mod4.gls,test)\nwrite.table(test, file=\"IndoreCaseStudyDataTEST5.csv\",sep=\",\")\n\n# Check the RMSE score of test dataset\nrmse(test$predicted.MEDV, predict(mod4.gls,test))\n","metadata":{"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"finalsubmission = data.frame(ID = sample$ID, MEDV = test$predicted.MEDV)\nhead(finalsubmission)","metadata":{"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"code","source":"write.csv(finalsubmission, \"submission.csv\", row.names = FALSE)","metadata":{"truste